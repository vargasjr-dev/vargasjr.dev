{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "entry": [
    "app/**/*.{ts,tsx}",
    "components/**/*.{ts,tsx}",
    "scripts/**/*.ts",
    "server/**/*.ts",
    "utils/**/*.ts",
    "terraform/main.ts"
  ],
  "project": [
    "app/**/*.{ts,tsx,js,jsx}",
    "components/**/*.{ts,tsx,js,jsx}",
    "scripts/**/*.{ts,tsx,js,jsx}",
    "server/**/*.{ts,tsx,js,jsx}",
    "utils/**/*.{ts,tsx,js,jsx}",
    "!**/*.test.{ts,tsx,js,jsx}",
    "!**/*.spec.{ts,tsx,js,jsx}",
    "!eslint-rules/**/*.js"
  ],
  "ignore": [
    "dist/**",
    "node_modules/**",
    "**/*.test.{ts,tsx,js,jsx}",
    "**/*.spec.{ts,tsx,js,jsx}",
    "eslint-rules/**/*.js",
    "drizzle.config.ts",
    "next.config.ts",
    "tailwind.config.ts",
    "vitest.config.ts",
    "knip.config.json"
  ],
  "ignoreDependencies": [
    "@stripe/stripe-js",
    "cors",
    "helmet",
    "twilio",
    "cdktf-cli",
    "constructs",
    "eslint",
    "eslint-config-next",
    "madge",
    "postcss-load-config"
  ],
  "ignoreBinaries": ["prettier"]
}
